{
  "name": "gerrit-tools",
  "displayName": "Gerrit Tools",
  "publisher": "ThomasFike",
  "repository": {
    "type": "git",
    "url": "https://github.com/ThomasFike/gerrit-tools"
  },
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "http://ThomasFike.com",
  "bugs": {
    "url": "https://github.com/ThomasFike/gerrit-tools/issues"
  },
  "description": "Adds tools for gerrit code review to vscode",
  "keywords": [
    "Gerrit",
    "Git"
  ],
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:gerrit-tools.pushToGerrit"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gerrit-tools.pushToGerrit",
        "title": "Push to Gerrit"
      }
    ],
    "configuration": {
      "title": "Gerrit Tools",
      "properties": {
        "gerrit-tools.push.defaultPushOption": {
          "description": "Specifies the first option when the prompt for when pushing",
          "type": "string",
          "default": "None",
          "enum": [
            "None",
            "Work in Progress",
            "Private",
            "Remove Work in Progress",
            "Remove Private"
          ]
        }
      }
    },
    "keybindings": [
      {
        "command": "gerrit-tools.pushToGerrit",
        "key": "ctrl+shift+g p"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
